  "private": true,
  "scripts": {
    "start": "npx cap serve",
    "run:web": "npx cap serve",
    "run:ios": "npm run build:ios && npx cap open ios",
    "run:ios:livereload": "npm run build:ios && ionic capacitor run ios --livereload",
    "run:android": "npm run build:android && npx cap open android",
    "run:android:livereload": "npm run build:android && ionic capacitor run android --livereload",
    "build:app": "ionic build && npx cap copy && npx cap copy ios && npx cap copy android",
    "build:android": "ionic build && npx cap copy && npx cap copy android",
    "build:ios": "ionic build && npx cap copy && npx cap copy ios",
    "resources": "cordova-res android --skip-config --copy && cordova-res ios --skip-config --copy",
    "update:project": "ionic capacitor sync",
    "info:capacitor": "npx cap doctor",
    "list:plugins": "npx cap ls",
    "test": "ng test",
    "test:coverage": "ng test --code-coverage"
  },
